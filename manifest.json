{
    "description": "Save links in the Open Read-Later specification format",
    "manifest_version": 2,
    "name": "Open Read-Later",
    "version": "1.0.0",
    "browser_action": {
        "browser_style": true,
        "default_popup": "src/html/popup.html",
        "default_icon": {
            "16": "icons/bookmark-16.png",
            "32": "icons/bookmark-32.png"
        }
    }
}
